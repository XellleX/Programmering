import java.awt.Graphics;

import javax.swing.JFrame;


public class Flaggor extends JFrame
{
	public Flaggor()
	{
		super("Hej");
		setBounds(500, 400, 100, 100);
		setDefaultCloseOperation(EXIT_ON_CLOSE);
		setVisible(true);
	}
	
	public void Paint(Graphics g)
	{
		g.drawRect(50, 50, 300, 350);
	}
}
