'************************************************************************

[definition]
Version=2.5
Revision=0
Author=Nick Bradbury
Icon=msie
NamedColors=1
SystemColors=1
RGBColors=1
SystemFonts=1

[at-rules]
@font-face=1
@import=1
@media=0
@page=0

[known types]
<units>				= (pt|in|pc|mm|cm|px|em|ex)
<generic-family>	= (serif|sans\-serif|cursive|fantasy|monospace)
<absolute-size> 	= (xx\-small|x\-small|small|medium|large|x\-large|xx\-large)
<relative-size>		= (larger|smaller)

[selectors]
Class=Y
ID=Y
Contextual=Y
Child=N
Descendant=Y
Sibling=Y
Attribute=N
Universal=N

[pseudo]
:link
:visited
:active
:hover

[groups]
Box
Classification
Color and Background
Font
Other
Positioning
Printing
Text

[properties]
background
background-attachment
background-color
background-image
background-position
background-repeat
border
border-bottom
border-bottom-color
border-bottom-style
border-bottom-width
border-color
border-left
border-left-color
border-left-style
border-left-width
border-right
border-right-color
border-right-style
border-right-width
border-style
border-top
border-top-color
border-top-style
border-top-width
border-width
clear
clip
color
cursor
display
filter
float
font
font-family
font-size
font-style
font-variant
font-weight
height
left
letter-spacing
line-height
list-style
list-style-image
list-style-position
list-style-type
margin
margin-bottom
margin-left
margin-right
margin-top
overflow
padding
padding-bottom
padding-left
padding-right
padding-top
page-break-after
page-break-before
position
text-align
text-decoration
text-indent
text-transform
top
unicode-bidi
vertical-align
visibility
width
z-index

[background]
group=Color and Background
value1=<background-color>
value2=<background-image>
value3=<background-repeat>
value4=<background-attachment>
value5=<background-position>

[background-attachment]
initial=scroll
group=Color and Background
value1=scroll|fixed

[background-color]
initial=transparent
group=Color and Background
value1=transparent|<color>

[background-image]
initial=none
group=Color and Background
value1=<url>|none

[background-position]
group=Color and Background
multiple=1
value1=(<percentage>|<length>)||(top|center|bottom)||(left|center|right)

[background-repeat]
initial=repeat
group=Color and Background
value1=repeat\-x|repeat\-y|no\-repeat|repeat

[border]
group=Box
value1=<border-width>
value2=<border-style>
value3=<color>

[border-bottom]
group=Box
value1=<border-bottom-width>
value2=<border-style>
value3=<border-bottom-color>

[border-bottom-color]
group=Box
value1=<color>

[border-bottom-style]
group=Box
value1=none|solid|double|groove|ridge|inset|outset

[border-bottom-width]
group=Box
value1=thin|medium|thick|<length>

[border-color]
group=Box
value1=<border-top-color>
value2=<border-right-color>
value3=<border-bottom-color>
value4=<border-left-color>

[border-left]
group=Box
value1=<border-left-width>
value2=<border-left-style>
value3=<border-left-color>

[border-left-color]
group=Box
value1=<color>

[border-left-style]
group=Box
value1=none|solid|double|groove|ridge|inset|outset

[border-left-width]
group=Box
value1=thin|medium|thick|<length>

[border-right]
group=Box
value1=<border-right-width>
value2=<border-right-style>
value3=<border-right-color>

[border-right-color]
group=Box
value1=<color>

[border-right-style]
group=Box
value1=none|solid|double|groove|ridge|inset|outset

[border-right-width]
group=Box
value1=thin|medium|thick|<length>

[border-style]
group=Box
value1=<border-top-style>
value2=<border-right-style>
value3=<border-bottom-style>
value4=<border-left-style>

[border-top]
group=Box
value1=<border-top-width>
value2=<border-top-style>
value3=<border-top-color>

[border-top-color]
group=Box
value1=<color>

[border-top-style]
group=Box
value1=none|solid|double|groove|ridge|inset|outset

[border-top-width]
group=Box
value1=thin|medium|thick|<length>

[border-width]
group=Box
value1=<border-top-width>
value2=<border-right-width>
value3=<border-bottom-width>
value4=<border-left-width>

[clear]
group=Box
value1=none|left|right|both

[clip]
group=Positioning
value1=<shape>|auto

[color]
group=Color and Background
value1=<color>

[cursor]
group=Other
multiple=1
initial=auto
value1=auto|crosshair|default|hand|move|e\-resize|ne\-resize|nw\-resize|n\-resize|se\-resize|sw\-resize|s\-resize|w\-resize|text|wait|help

[display]
group=Classification
value1=none|table\-header\-group|table\-footer\-group

[filter]
group=Other
multiple=1
value1=<ie4filter>

[float]
group=Box
initial=none
value1=left|right|none

[font]
inherited=1
group=Font
value1=<font-style>
value2=<font-variant>
value3=<font-weight>
value4=<font-size>
value5=<line-height>
value6=<font-family>

[font-family]
inherited=1
group=Font
value1=((<generic-family>|<family-name>),)*(<generic-family>|<family-name>)

[font-size]
inherited=1
initial=medium
group=Font
value1=<relative-size>|<absolute-size>|<percentage>|<length>

[font-style]
group=Font
value1=normal|italic|oblique

[font-variant]
group=Font
value1=normal|small\-caps

[font-weight]
group=Font
value1=normal|bold|100|200|300|400|500|600|700|800|900

[height]
group=Positioning
initial=auto
value1=auto|<percentage>|<length>

[left]
group=Positioning
value1=auto|<percentage>|<length>

[letter-spacing]
group=Text
initial=normal
value1=normal|<length>

[line-height]
group=Text
value1=normal|<percentage>|<length>|<number>

[list-style]
inherited=1
initial=
group=Classification
value1=<list-style-type>
value2=<list-style-position>
value3=<list-style-image>

[list-style-image]
inherited=1
initial=none
group=Classification
value1=<url>|none

[list-style-position]
inherited=1
initial=outside
group=Classification
value1=inside|outside

[list-style-type]
inherited=1
initial=disc
group=Classification
value1=disc|circle|square|decimal|lower\-roman|upper\-roman|lower\-alpha|upper\-alpha|none

[margin]
inherited=0
initial=
group=Box
value1=<margin-top>
value2=<margin-right>
value3=<margin-bottom>
value4=<margin-left>

[margin-bottom]
inherited=0
initial=0
group=Box
value1=<percentage>|<length>|auto

[margin-left]
inherited=0
initial=0
group=Box
value1=<percentage>|<length>|auto

[margin-right]
inherited=0
initial=0
group=Box
value1=<percentage>|<length>|auto

[margin-top]
inherited=0
initial=0
group=Box
value1=<percentage>|<length>|auto

[overflow]
group=Positioning
initial=visible
value1=visible|hidden|scroll|auto

[overflow-x]
group=Positioning
initial=visible
value1=visible|hidden|scroll|auto

[overflow-y]
group=Positioning
initial=visible
value1=visible|hidden|scroll|auto

[padding]
inherited=0
initial=
group=Box
value1=<padding-top>
value2=<padding-right>
value3=<padding-bottom>
value4=<padding-left>

[padding-bottom]
inherited=0
initial=0
group=Box
value1=<percentage>|<length>

[padding-left]
inherited=0
initial=0
group=Box
value1=<percentage>|<length>

[padding-right]
inherited=0
initial=0
group=Box
value1=<percentage>|<length>

[padding-top]
inherited=0
initial=0
group=Box
value1=<percentage>|<length>

[page-break-after]
group=Printing
initial=auto
value1=auto|always

[page-break-before]
group=Printing
initial=auto
value1=auto|always

[position]
group=Positioning
initial=static
value1=absolute|relative|static

[text-align]
group=Text
value1=left|right|center|justify

[text-decoration]
group=Text
initial=None
multiple=1
value1=none|underline|overline|line\-through

[text-indent]
group=Text
value1=<percentage>|<length>

[text-transform]
group=Text
initial=none
value1=capitalize|uppercase|lowercase|none

[top]
group=Positioning
initial=auto
value1=<percentage>|<length>|auto

[unicode-bidi]
inherited=0
initial=normal
group=Text
value1=normal|embed|bidi\-override

[vertical-align]
group=Text
initial=baseline
value1=baseline|sub|super|top|middle|bottom|text\-top|text\-bottom

[visibility]
group=Positioning
initial=inherit
value1=visible|hidden|inherit

[width]
group=Positioning
initial=auto
value1=auto|<percentage>|<length>

[z-index]
group=Positioning
initial=auto
value1=auto|<integer>

[browser-tips]
property = word-spacing
message	= The property "word-spacing" is only supported by IE4 on the Mac
property = float
message = Using "float" in IE4 may lead to unpredictable results.
property = clear
message = Using "clear" in IE4 may lead to unpredictable results.
